@{
    ViewData["Title"] = "Trgovine";
}

<div id="id_store_list_div">

</div>

@section Scripts{
    <script>
        $(document).ready(function () {


            $("#id_store_list").replaceWith(
                $.ajax({
                    url: "@Url.Action("List")",
                    method: "GET",
                    success: function (html) {
                        $("#id_store_list_div").html(html);
                    }
                })
            );

        });





    </script>
}
